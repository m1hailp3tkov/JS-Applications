<div class="col-md-6 text-center col-lg">
    <img class="profile-img" src="./images/user.png" />
    <div class="profile-info">
      <p>Username: <small>{{userInfo.username}}</small></p>
      <p class="infoType">Organizer of {{eventCount}} events.</p>
      {{#each events}}
      <p>{{this}}</p>
      {{/each}}
    </div>
  </div>